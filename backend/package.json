{
  "name": "techint",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "lint": "lint-staged",
    "lint-all": "pretty-quick && eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "lint-staged": {
    "*.js": [
      "pretty-quick --staged",
      "eslint --fix"
    ],
    "*.+(json|css|md)": [
      "pretty-quick --staged"
    ]
  },
  "author": "Abdulsalam Suliyat <sullieart@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.25.0",
    "cloudmersive-image-api-client": "^1.3.5",
    "connect-mongo": "^4.6.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "express": "~4.17.2",
    "express-session": "^1.17.2",
    "http-errors": "~1.6.3",
    "mongoose": "^6.1.7",
    "mongoose-autopopulate": "^0.16.0",
    "morgan": "~1.9.1",
    "passport": "^0.5.2",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^6.1.0",
    "pug": "2.0.0-beta11",
    "socket.io": "^4.4.1"
  },
  "devDependencies": {
    "connect-livereload": "^0.6.1",
    "eslint": "^7.2.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.5",
    "eslint-plugin-react-hooks": "^4.0.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "livereload": "^0.9.1",
    "prettier": "2.0.5",
    "pretty-quick": "^2.0.1"
  }
}
